{
  "manifest_version": 2,
  "name": "Bookmark Manager",
  "description": "Mark your website",
  "version": "2.0.1",
  "icons":{
    "128":"icon_128.png",
    "48":"icon_48.png",
    "16":"icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Bookmark Manager"
  },
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"]
  }
}